gitlab-runner ALL=(ALL) NOPASSWD: /bin/systemctl start kudubot-telegram.service
gitlab-runner ALL=(ALL) NOPASSWD: /bin/systemctl stop kudubot-telegram.service
gitlab-runner ALL=(ALL) NOPASSWD: /bin/systemctl restart kudubot-telegram.service
